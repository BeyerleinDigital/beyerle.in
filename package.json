{
  "name": "beyerlein-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "postbuild:prod": "gzipper c --remove-larger --exclude jpeg,jpg,png,webp ./dist/beyerlein-website/browser && gzipper c --brotli --remove-larger --exclude jpeg,jpg,png,webp ./dist/beyerlein-website/browser",
    "build:prod:test": "ng build --configuration=prodTest",
    "postbuild:prod:test": "gzipper c --remove-larger --exclude jpeg,jpg,png,webp ./dist/beyerlein-website/browser && gzipper c --brotli --remove-larger --exclude jpeg,jpg,png,webp ./dist/beyerlein-website/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "release": " npx standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.1",
    "@angular/common": "^18.1.1",
    "@angular/compiler": "^18.1.1",
    "@angular/core": "^18.1.1",
    "@angular/forms": "^18.1.1",
    "@angular/platform-browser": "^18.1.1",
    "@angular/platform-browser-dynamic": "^18.1.1",
    "@angular/platform-server": "^18.1.1",
    "@angular/router": "^18.1.1",
    "@angular/ssr": "^18.0.7",
    "express": "^4.19.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular-devkit/core": "^18.0.7",
    "@angular-devkit/schematics": "^18.0.7",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.6",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@typescript-eslint/utils": "^7.2.0",
    "eslint": "^8.57.0",
    "gzipper": "^8.0.1",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.5"
  }
}
